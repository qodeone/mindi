{#
/**
 * @file
 * Theme override for tabs.
 */
#}
{%
  set classes = [
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
  ]
%}
<div{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {% if label %}
    <div{{ title_attributes.addClass( 'uk-nav-header' ) }}>{{ label }}</div>
  {% endif %}
  {{ title_suffix }}
  {% block content %}
    {% if content %}
      <nav class="tabs uk-overflow-hidden" role="navigation" aria-label="{{ 'Tabs'|t }}">
        {{ content }}
      </nav>
    {% endif %}
  {% endblock %}
</div>
